class BreadRoll {
  bake() {
    print "Shape and bake in the oven.";
  }
}

class Bagel < BreadRoll {
  init() {
    this.seasoning = "salt";
  }

  eat() {
    print "Crunch crunch crunch, it's delicious with " + this.seasoning + ".";
  }
}
var bagel = Bagel();
bagel.bake();


bagel.seasoning = "salt and pepper";
bagel.eat();
